import static java.lang.System.*;

public class Triples
{
   private int number;

	public Triples()
	{
		this(0);
	}

	public Triples(int num)
	{
		setNum(num);
	}

	public void setNum(int num)
	{
		number = num;
	}
	
	private int greatestCommonFactor(int a, int b, int c)
	{
		//int max = 0;
		
		while (b > 0)
	    {
	        int d = b;
	        b = a % b; // % is remainder
	        a = d;
	    }
		
		while (c > 0)
	    {
	        int d = c;
	        c = a % c; // % is remainder
	        a = d;
	    }
		
	    return a;
	}

	public String toString()
	{
		int a, b, c;
		for(int i = 1; i < number; i++){
			a = i;
			
		}
		String output="";






		return output+"\n";
	}
}