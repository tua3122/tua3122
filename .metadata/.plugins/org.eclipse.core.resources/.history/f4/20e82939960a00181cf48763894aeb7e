import java.util.*;
import java.math.*;

public class Circle
{
	private double radius;
	private double area;

	public void setRadius(double rad)
	{
		Scanner keyboard = new Scanner(System.in);
		radius = keyboard.nextDouble();
		keyboard.close();
	}

	public void calculateArea( )
	{
		double area = Math.PI * radius * radius;
	}

	public void print( )
	{
	}
}